{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "no-unused-vars": "off",
        "arrow-body-style": "off",
        "indent": "off",
        "comma-dangle": "off",
        "quotes": "off",
        "keyword-spacing": "off",
        "eol-last": "off",
        "no-multiple-empty-lines": "off",
        "padded-blocks": "off",
        "object-curly-newline": "off",

        "no-const-assign": "off",
        "no-return-assign": "off",
        "no-trailing-spaces": "off",
        "max-len": "off",

        "space-before-blocks": "off",
        "array-callback-return": "off",
        "key-spacing": "off",
        "eqeqeq": "off",
        "no-unused-expressions": "off",
        "no-irregular-whitespace": "off",
        "object-shorthand": "off"
    }
}
